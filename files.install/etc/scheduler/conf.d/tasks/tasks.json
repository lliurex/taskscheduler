{
	"Maintenance":
	{
		"Update mirror":"lliurex-mirror unattendedupdate llx16",
		"Update Lliurex":"lliurex-upgrade -u",
		"Shutdown":"shutdown -h"
	},
	"Utilities":
	{
		"Notification":"notify-send -u critical -i taskscheduler -t 0 %s",
		"Play sound":"ffplay -nodisp %s",
		"Update Lliurex Store cache":"lliurex-store-cli --update"
	}
}
